<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.maua.ControleInterface">
   <children>
      <VBox alignment="TOP_CENTER" layoutX="14.0" layoutY="14.0" prefHeight="294.0" prefWidth="321.0" spacing="16.0">
         <children>
            <Label fx:id="lblSaida" text="Saida" />
            <TextField fx:id="txtEntrada" />
            <Button fx:id="btnButton" mnemonicParsing="false" onAction="#exibirNome" text="Ok" />
            <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0" spacing="24.0">
               <children>
                  <VBox prefHeight="100.0" prefWidth="212.0">
                     <children>
                        <TextField fx:id="txtNumero1" />
                        <Button mnemonicParsing="false" onAction="#somar" text="Button" />
                        <TextField id="txtNumero2" fx:id="txtNumero2" alignment="TOP_LEFT" />
                     </children>
                  </VBox>
               </children>
               <padding>
                  <Insets top="16.0" />
               </padding>
            </HBox>
         </children>
         <padding>
            <Insets bottom="8.0" right="8.0" top="8.0" />
         </padding>
      </VBox>
      <VBox layoutX="361.0" layoutY="23.0" prefHeight="353.0" prefWidth="212.0" spacing="8.0">
         <children>
            <Label text="Cadastro" />
            <TextField fx:id="txtNome" promptText="Nome:" />
            <TextField fx:id="txtSenha" promptText="Senha:" />
            <TextField fx:id="txtUrl" promptText="URL:" />
            <Button fx:id="btnPreview" mnemonicParsing="false" onAction="#previewPhoto" text="Preview foto" />
            <ImageView fx:id="imgFoto" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
            <Button mnemonicParsing="false" onAction="#cadastrar" text="Cadastrar" />
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
